<f:DialogWindowBase x:Class="prbd_2324_a03.View.DialogDeleteTricount"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:f="clr-namespace:PRBD_Framework;assembly=PRBD_Framework"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prbd_2324_a03.View"
        xmlns:vm="clr-namespace:prbd_2324_a03.ViewModel"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance vm:DialogDeleteTricountViewModel}"
        Title="DialogDeleteTricount"   WindowStartupLocation="CenterScreen" Height="200" Width="400">
    <DockPanel>
        <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Content="Yes" Width="50" Command="{Binding YesCommand}" Margin="10" IsDefault="True"/>
            <Button Content="No" Width="50" Command="{Binding NoCommand}" Margin="10" IsCancel="True" />
        </StackPanel>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock HorizontalAlignment="Center" Text="You're about to delete this Tricount"/>
            <TextBlock HorizontalAlignment="left" Text="Do you Confirm ?"/>
        </StackPanel>
    </DockPanel>
</f:DialogWindowBase>
